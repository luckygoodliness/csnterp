<?xml version="1.0" encoding="UTF-8"?>
<commonquery version="1.0">
    <querysql>
        <daotype>card-dao</daotype>
        <daokey>common_query_prm_purchase_req_detail_grid_use</daokey>
    </querysql>
    <defaultSort>check_date desc, seq_no asc</defaultSort>
    <conditions columns="4">
        <JText pos="A1:A1" cid="purchaseReqNo" fieldLabel_s="purchaseReqNo" field="purchase_req_no"/>
        <JText pos="A2:A2" cid="name" fieldLabel_s="assetName" field="name"/>
        <JText pos="B1:B1" cid="model" fieldLabel_s="model" field="model"/>
        <JText pos="B2:B2" cid="factory" fieldLabel_s="factoryName" field="factory"/>
    </conditions>
    <quickquery>
        <QueryCids>purchaseReqNo,name,model,factory</QueryCids>
        <ForceQueryCids></ForceQueryCids>
    </quickquery>
    <results>
        <JTextCol text_s="purchaseReqNo" dataIndex="purchaseReqNo" width_n="100"/>
        <JTextCol text_s="assetName" dataIndex="name" width_n="100"/>
        <JTextCol text_s="model" dataIndex="model" width_n="100"/>
        <JTextCol text_s="factoryName" dataIndex="factory" width_n="100"/>
    </results>
</commonquery>