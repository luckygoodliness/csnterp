<?xml version="1.0" encoding="UTF-8"?>
<commonquery version="1.0">
    <querysql>
        <daotype>certificate-dao</daotype>
        <daokey>common_query_currtype</daokey>
    </querysql>
    <defaultSort>currency_code asc</defaultSort>
    <conditions columns="4">
        <JText pos="A1:A1" cid="currencyCode" fieldLabel_s="currencyCode" field="currency_code"/>
        <JText pos="A2:A2" cid="currencyDesc" fieldLabel_s="currencyDesc" field="currency_desc"/>
    </conditions>
    <results>
        <JTextCol text_s="currencyCode" dataIndex="currencyCode" width_n="100"/>
        <JTextCol text_s="currencyDesc" dataIndex="currencyDesc" width_n="100"/>
    </results>
</commonquery>