<?xml version="1.0" encoding="UTF-8"?>
<commonquery version="1.0">
    <querysql>
        <daotype>card-dao</daotype>
        <daokey>common_query_prm_purchase_req_detail</daokey>
    </querysql>
    <defaultSort>check_date desc, seq_no asc</defaultSort>
    <conditions columns="3">
        <JText pos="A1:A1" cid="purchaseReqNo" fieldLabel_s="purchaseReqNo" field="purchase_req_no"/>
        <JText pos="A2:A2" cid="departmentName" fieldLabel_s="departmentName" field="department_name"/>
        <JText pos="B1:B1" cid="subjectName" fieldLabel_s="subjectName" field="subject_name"/>
        <JText pos="B2:B2" cid="name" fieldLabel_s="name" field="name"/>
        <JText pos="C1:C1" cid="model" fieldLabel_s="model" field="model"/>
        <JText pos="C2:C2" cid="factory" fieldLabel_s="factoryName" field="factory"/>
    </conditions>
    <results>
        <JTextCol text_s="purchaseReqNo" dataIndex="purchaseReqNo" width_n="100"/>
        <JTextCol text_s="departmentName" dataIndex="departmentName" width_n="100"/>
        <JTextCol text_s="subjectName" dataIndex="subjectName" width_n="100"/>
        <JTextCol text_s="name" dataIndex="name" width_n="100"/>
        <JTextCol text_s="model" dataIndex="model" width_n="100"/>
        <JTextCol text_s="factoryName" dataIndex="factory" width_n="100"/>
        <JTextCol text_s="amount" dataIndex="amount" width_n="100"/>
        <JTextCol text_s="budgetPrice" dataIndex="budgetPrice" width_n="100"/>
        <JTextCol text_s="supplierId" dataIndex="supplierId" width_n="100"/>
        <JTextCol text_s="descp" dataIndex="remark" width_n="100"/>
    </results>
</commonquery>