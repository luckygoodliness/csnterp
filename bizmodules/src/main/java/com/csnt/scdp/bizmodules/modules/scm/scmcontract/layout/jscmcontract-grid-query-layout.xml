<?xml version="1.0" encoding="UTF-8"?>
<commonquery version="1.0">
    <querysql>
        <daotype>scmcontract-dao</daotype>
        <daokey>contract_grid_query</daokey>
    </querysql>
    <defaultSort>create_time desc</defaultSort>
    <conditions columns="4">
        <JText pos="A1:A1" fieldLabel_s="scmContractCode" cid="scmContractCode" field="scm_Contract_Code"/>
        <JText pos="B1:B1" fieldLabel_s="supplierName" cid="supplierName" field="supplier_Name"/>
        <JHidden pos="B1:B1" fieldLabel_s="supplierCode" cid="supplierCode" field="supplier_Code"/>
        <JHidden pos="B1:B1" fieldLabel_s="supplierCodeS" cid="supplierCodeS" field="supplier_Code_S"/>
        <JText pos="C1:C1" fieldLabel_s="projectId" cid="projectId" field="project_Id"/>
        <JText pos="D1:D1" fieldLabel_s="departmentCode" cid="departmentCode" field="department_Code"/>
    </conditions>
    <quickquery>
        <QueryCids>scmContractCode</QueryCids>
        <ForceQueryCids></ForceQueryCids>
    </quickquery>
    <results>
        <JTextCol text_s="scmContractCode" dataIndex="scmContractCode" width_n="120"/>
        <JTextCol text_s="contractNature" dataIndex="contractNatureS" width_n="120"/>
        <JDecCol text_s="contractAmount" dataIndex="amount" width_n="150"/>
        <JTextCol text_s="projectName" dataIndex="projectName" width_n="120"/>
        <JTextCol text_s="departmentCode" dataIndex="departmentCode" width_n="120"/>
        <JTextCol text_s="supplierName" dataIndex="supplierName" width_n="120"/>
        <JDatetimeCol text_s="effectiveDate" dataIndex="effectiveDate" width_n="150"/>
        <JDatetimeCol text_s="stampDate" dataIndex="stampDate" width_n="150"/>
        <JTextCol text_s="contractState" dataIndex="contractStateCombo" width_n="120"/>
        <JTextCol text_s="isVirtual" dataIndex="isVirtualCombo" width_n="120"/>
        <JTextCol text_s="taxRegistrationNo" dataIndex="taxRegistrationNo" width_n="120" hidden_b="true"/>
        <JDecCol text_s="paidRt" dataIndex="paidRt" width_n="150"/>
        <JDecCol text_s="invoiceAmountRt" dataIndex="invoiceAmountRt" width_n="150"/>
        <JTextCol text_s="remark" dataIndex="remark" width_n="120"/>

    </results>
</commonquery>